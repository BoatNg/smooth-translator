{
  "manifest_version": 2,

  "name": "TransIt",
  "description": "划词翻译",
  "version": "1.0",
  
  "icons": {
    "48": "icon48.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["css/transit.css"],
      "js": ["js/transit.js"]
    }
  ],
  "permissions": [
    "tabs"
  ]
}